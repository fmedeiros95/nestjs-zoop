{
  "name": "@fmedeiros95/nestjs-zoop",
  "version": "1.0.4",
  "private": false,
  "description": "NestJS - Zoop module",
  "keywords": [
    "nest",
    "zoop",
    "nodejs"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "deploy": "sh ./publish.sh",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "major": "npm run release -- --release-as major",
    "minor": "npm run release -- --release-as minor",
    "patch": "npm run release -- --release-as patch",
    "release": "standard-version",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmedeiros95/nestjs-zoop.git"
  },
  "author": "Felipe Medeiros <medeiros.dev@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fmedeiros95/nestjs-zoop/issues"
  },
  "homepage": "https://github.com/fmedeiros95/nestjs-zoop#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@nestjs/common": "^9.3.12",
    "@nestjs/core": "^9.3.12",
    "@nestjs/testing": "^9.3.12",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^5.57.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "axios": "^1.3.4",
    "qs": "^6.11.1"
  }
}
